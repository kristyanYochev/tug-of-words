<Window x:Class="frontend.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:frontend"
        xmlns:view="clr-namespace:frontend.View"
        xmlns:ViewModel="clr-namespace:frontend.ViewModel"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800"
        Background="Black">
    <Viewbox Stretch="Uniform">
        <Grid Width="400" Height="300" Background="White"
              x:Name="grid"/>
        <!--<ItemsControl Width="400" Height="300" Background="White"
                      ItemsSource="{Binding RenderedElems}"/>-->
    </Viewbox>
</Window>
